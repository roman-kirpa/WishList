{
    "name": "Wish List",
    "description": "take url and paste to wishlist",
    "browser_action" : {
	"default_icon":"icon.png",
	"default_title":"wishlist",
	"default_popup": "menu.html"
	},
    "manifest_version": 2,
    "update_url": "http://clients2.google.com/service/update2/crx",
    "version": "0.1",
    "icons": {
        "128": "icon.png"
    },
	"permissions": [
		"storage",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>",
		"tabs",
		"notifications",
        "http://*/",
        "https://*/"
	],
 "background" : {
	"scripts" :["jquery-3.3.1.js","goToSite.js", "AddToWishList.js"]
	}
}
